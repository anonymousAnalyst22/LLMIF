     89          /*************************************************************************************************
     90           *                    LCD EMULATION FUNCTIONS
     91           *
     92           * Some evaluation boards are equipped with Liquid Crystal Displays
     93           * (LCD) which may be used to display diagnostic information. These
     94           * functions provide LCD emulation, sending the diagnostic strings
     95           * to Z-Tool via the RS232 serial port. These functions are enabled
     96           * when the "LCD_SUPPORTED" compiler flag is placed in the makefile.
     97           *
     98           * Most applications update both lines (1 and 2) of the LCD whenever
     99           * text is posted to the device. This emulator assumes that line 1 is
    100           * updated first (saved locally) and the formatting and send operation
    101           * is triggered by receipt of line 2. Nothing will be transmitted if
    102           * only line 1 is updated.
    103           *
    104           *************************************************************************************************/
    105          
    106          
    107          /**************************************************************************************************
    108           * @fn      HalLcdWriteString
    109           *
    110           * @brief   Write a string to the LCD
    111           *
    112           * @param   str    - pointer to the string that will be displayed
    113           *          option - display options
    114           *
    115           * @return  None
    116           **************************************************************************************************/

   \                                 In  segment BANKED_CODE, align 1, keep-with-next
    117          void HalLcdWriteString ( char *str, uint8 option)
   \                     HalLcdWriteString:
    118          {
   \   000000                ; Saved register size: 0
   \   000000                ; Auto size: 0
    119          #if (HAL_LCD == TRUE)
    120            uint8 strLen = 0;
    121            uint8 lcdShow[HAL_LCD_MAX_CHARS+1];
    122          
    123            /* Check boundries */
    124            strLen = (uint8)osal_strlen( (char*)str );
    125            if ( strLen > HAL_LCD_MAX_CHARS )
    126            {
    127              strLen = HAL_LCD_MAX_CHARS;
    128            }
    129            
    130            /* Clear buffer */
    131            for(uint8 i = 0; i < HAL_LCD_MAX_CHARS; i++)
    132            {
    133              lcdShow[i] = ' ';
    134            }
    135            lcdShow[HAL_LCD_MAX_CHARS] = '\0';
    136            
    137            /* Display the string */
    138            osal_memcpy( lcdShow, str, strLen );
    139            HalLcd_HW_WriteLine(option, (const uint8 *)lcdShow);
    140          #endif //HAL_LCD
    141          }
